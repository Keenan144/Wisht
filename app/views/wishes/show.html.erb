<% provide(:title, @wish.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        Wish Information
      </h1>

      <div class="wish">
        <div class="title">
          <%= @wish.content %>
          <br>
        </div>
